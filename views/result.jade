extends common/layout

block style
  link(rel="stylesheet", href="/styles/result.css")

block content
  section(class="container-fluid")
    table(class="table table-striped tablesorter")
      thead
        tr
          th クエリ
          th クリック数
          th 表示回数
          th CTR
      tbody
        - for (var i = 0; i < data.length; i++)
          tr
            td= data[i].word
            td= data[i].clickNum
            td= data[i].dispNum
            td= data[i].ctr

block script
  script(type="text/javascript", src="/components/jquery.tablesorter/jquery.tablesorter.min.js")
  script(type="text/javascript", src="/scripts/result.js")
